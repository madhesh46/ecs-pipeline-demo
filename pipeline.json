{
  "pipeline": {
    "name": "ecs-pipeline",
    "roleArn": "arn:aws:iam::699582537231:role/ecsCodePipelineServiceRole",
    "artifactStore": {
      "type": "S3",
      "location": "ecs-pipeline-artifacts-699582537231"
    },
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "name": "GitHub_Source",
            "actionTypeId": {
              "category": "Source",
              "owner": "ThirdParty",
              "provider": "GitHub",
              "version": "1"
            },
            "configuration": {
              "Owner": "madhesh46",
              "Repo": "ecs-pipeline-demo",
              "Branch": "main",
              "PollForSourceChanges": "true"
            },
            "outputArtifacts": [
              {
                "name": "SourceArtifact"
              }
            ],
            "runOrder": 1,
            "region": "ap-south-1"
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "name": "BuildAction",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "configuration": {
              "ProjectName": "ecs-build-project"
            },
            "inputArtifacts": [
              {
                "name": "SourceArtifact"
              }
            ],
            "outputArtifacts": [
              {
                "name": "BuildArtifact"
              }
            ],
            "runOrder": 1,
            "region": "ap-south-1"
          }
        ]
      },
      {
        "name": "Deploy",
        "actions": [
          {
            "name": "DeployToECS",
            "actionTypeId": {
              "category": "Deploy",
              "owner": "AWS",
              "provider": "ECS",
              "version": "1"
            },
            "configuration": {
              "ClusterName": "ecs-demo-cluster",
              "ServiceName": "ecs-demo-ec2-service",
              "FileName": "imagedefinitions.json"
            },
            "inputArtifacts": [
              {
                "name": "BuildArtifact"
              }
            ],
            "outputArtifacts": [],
            "runOrder": 1,
            "region": "ap-south-1"
          }
        ]
      }
    ],
    "version": 1
  }
}

