{
  "pipeline": {
    "name": "ecs-pipeline",
    "roleArn": "arn:aws:iam::699582537231:role/ecsCodePipelineRole",
    "artifactStore": {
      "type": "S3",
      "location": "ecs-pipeline-artifacts-699582537231"
    },
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "name": "SourceAction",
            "actionTypeId": {
              "category": "Source",
              "owner": "AWS",
              "provider": "CodeStarSourceConnection",
              "version": "1"
            },
            "outputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ],
            "configuration": {
              "ConnectionArn": "arn:aws:codeconnections:ap-south-1:699582537231:connection/995914b4-21d6-45eb-9e65-f35cb260fb12",
              "FullRepositoryId": "madhesh46/ecs-pipeline-demo",
              "BranchName": "main",
              "OutputArtifactFormat": "CODE_ZIP"
            },
            "runOrder": 1
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "name": "BuildAction",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "inputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ],
            "outputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ],
            "configuration": {
              "ProjectName": "ecs-build-project"
            },
            "runOrder": 1
          }
        ]
      },
      {
        "name": "Deploy",
        "actions": [
          {
            "name": "DeployAction",
            "actionTypeId": {
              "category": "Deploy",
              "owner": "AWS",
              "provider": "ECS",
              "version": "1"
            },
            "inputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ],
            "configuration": {
              "ClusterName": "ecs-demo-cluster",
              "ServiceName": "ecs-demo-ec2-service"
            },
            "runOrder": 1
          }
        ]
      }
    ],
    "version": 1
  }
}

