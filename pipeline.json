{
  "pipeline": {
    "name": "ecs-pipeline",
    "roleArn": "arn:aws:iam::699582537231:role/AWS-CodePipeline-Service",
    "artifactStore": {
      "type": "S3",
      "location": "ecs-pipeline-artifacts-699582537231"
    },
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "name": "Source",
            "actionTypeId": {
              "category": "Source",
              "owner": "ThirdParty",
              "provider": "GitHub",
              "version": "1"
            },
            "outputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ],
            "configuration": {
              "Owner": "madhesh46",
              "Repo": "ecs-pipeline-demo",
              "Branch": "main",
              "OAuthToken": "<GITHUB_TOKEN>"
            },
            "runOrder": 1
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "name": "Build",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "inputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ],
            "outputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ],
            "configuration": {
              "ProjectName": "ecs-build-project"
            },
            "runOrder": 1
          }
        ]
      },
      {
        "name": "Deploy",
        "actions": [
          {
            "name": "Deploy",
            "actionTypeId": {
              "category": "Deploy",
              "owner": "AWS",
              "provider": "ECS",
              "version": "1"
            },
            "inputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ],
            "configuration": {
              "ClusterName": "ecs-demo-cluster",
              "ServiceName": "ecs-demo-ec2-service",
              "FileName": "imagedefinitions.json"
            },
            "runOrder": 1
          }
        ]
      }
    ],
    "version": 1
  }
}

